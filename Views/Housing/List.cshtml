@model IEnumerable<Booking.web.Models.HousingViewModel>

<div class="container mt-4">
    <h2>@Localizer["StaysDestinations"]</h2>
    <div class="row">
        @foreach (var house in Model)
        {
                <div class="col-md-6 mb-4">
                    <partial name="_HousingCards" model="house" />
                </div>
        }
    </div>
</div>